{
  "editor.rulers": [
    120
  ],
  "files.eol": "\n",
  "files.associations": {
    "chrono": "cpp",
    "*.pyx": "python",
    "cmath": "cpp",
    "algorithm": "cpp",
    "format": "cpp",
    "xiosbase": "cpp",
    "xlocale": "cpp",
    "xutility": "cpp"
  },
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[markdown]": {
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "editor.insertSpaces": true,
  },
  "editor.tabSize": 2,
  "editor.insertSpaces": true
}